{
    "version": 2,
    "build": [
        {
            "src": "main.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "devCommand": "uvicorn main:app --host 0.0.0.0 --port 3000",
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ]
}